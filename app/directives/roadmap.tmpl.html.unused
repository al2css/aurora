<div class="roadmap flight{{ flights.id }}" id="flightLegId{{ flights.id }}" ng-repeat="flights in roadmap.flights.assigned_flights | orderBy:'departingTime'" ng-style="roadmap.setFlightToHour(flights.departingTime)">
	<q></q>
	<h2>
		<a href="javascript:;" title="" tooltip-placement="top" tooltip-html-unsafe="Flight: <b>{{ flights.from }} - {{ flights.to }}</b><br/> Departing time: <b>{{ flights.departingTime * 1000 | date:'MMM d, y HH:mm' }}</b>" ng-click="goToFlight(flights.id);">{{ flights.fromCode }} - {{ flights.toCode }}</a>
	</h2>
	<div class="clear"></div>
</div>
